<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <!-- Compiled and minified CSS -->
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <title>Register</title>
</head>
<body>
  
  <nav class="teal">
    <div class="nav-wrapper container">
      <a href="/home" class="brand-logo center">AirBnB</a>
      <ul id="nav-mobile" class="left hide-on-med-and-down">
    
        <li><a href="/map">Our Location</a></li>
      </ul>
    </div>
  </nav>
                <form action="/register" method="POST">
                    <div class="container">
                        <div class="row">
                            <div class="col s12 m7 offset-l2">
                                <h4 class="center">Register Account</h4>
                                <div class="card-panel">
                                       <!-- user -->
                                <div>
                                    <label for="name">Name</label>
                                    <input type="text" name="name" id="name" required>
                                </div>
                                    
                                <!-- email -->
                                <div>
                                <label for="name">Email</label>
                                <input type="email" name="email" id="email" required>
                            </div>
                                    <!-- password -->
                                    <div>
                                <label for="password">Password</label>
                                <input type="password" name="password" id="password" required>
                            </div>
                            <p class="center">Already have account? Login here <a href="/login">Login</a></p>
                           <button type="submit" class="btn">Register</button>
                                </div>
                            </div>
                        </div>
                    </div>              
                </form>

                <footer>
                  <%- include('common/footer') %> 
                </footer>
</body>
</html>